{
	"name": "Monokai Dimmed Light",
	"type": "light",
	"colors":
	{
		"editor.background": "#fdf6e3",
		"editor.foreground": "#545454",
		"activityBarBadge.background": "#304db9",
		"sideBarTitle.foreground": "#797878",
		"activityBar.background": "#eee8d5",
		"activityBar.foreground": "#3c3c3c",
		"sideBar.background": "#dfd9c6",
		"scrollbarSlider.background": "#93a1a1",
		"menu.selectionBackground": "#cb4b1677",
		"menu.selectionBorder": "#dc322f",
		"menu.background": "#fef8f5",
		"titleBar.activeBackground": "#c9c4b2",
		"titleBar.inactiveBackground": "#eee8d5",
		"tab.activeBackground": "#2aa19840",
		"tab.inactiveBackground": "#2aa19810",
		"tab.hoverBackground": "#6c71c460",
		"statusBar.background": "#e4dec3",
		"statusBar.foreground": "#002b36",
		"statusBar.noFolderBackground": "#1acf9f9d",
		"statusBar.noFolderForeground": "#002b36",
		"statusBar.debuggingForeground": "#0dff00",
		"statusBar.debuggingBackground": "#d26600",

		"editorWidget.background": "#eee8d5",
		"editorSuggestWidget.selectedBackground": "#2aa198",
		"list.activeSelectionBackground": "#6c71c4",
		"list.hoverBackground": "#93a1a140",
		"editorGroupHeader.tabsBackground": "#eee8d5"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9A9B99"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#6089B4"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#9872A2"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#9872A2"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#CE6700"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#9872A2"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#6089B4"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#9AA83A"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#9B0000"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
